{
  "name": "@empoleon/solid-floating-ui",
  "version": "0.0.6",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:js": "rollup -c --no-sourcemap",
    "build": "yarn clean && yarn build:types && yarn build:js",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run build:prod",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "biome lint",
    "storybook": "storybook dev -p 6006",
    "newchangeset": "yarn exec changeset",
    "version": "yarn changeset version",
    "release": "yarn changeset publish",
    "version:dev": "yarn version prerelease",
    "release:dev": "yarn npm publish --tag dev"
  },
  "sideEffects": false,
  "keywords": [
    "solid",
    "solidjs",
    "solid-js",
    "positioning",
    "tooltip",
    "floating-ui"
  ],
  "author": "Paul M",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulm17/floating-ui-solid"
  },
  "license": "MIT",
  "description": "SolidJS bindings for Floating UI",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-external-helpers": "^7.16.7",
    "@babel/plugin-proposal-do-expressions": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.16.9",
    "@biomejs/biome": "^1.9.4",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "^2.29.5",
    "@changesets/get-release-plan": "3.0.17",
    "@changesets/types": "5.2.1",
    "@eslint/js": "^9.23.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^12.1.4",
    "@solid-primitives/resize-observer": "^2.1.3",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-a11y": "^9.0.15",
    "@storybook/addon-links": "^9.0.15",
    "@storybook/addon-vitest": "^9.0.15",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^27.4.6",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-preset-solid": "^1.9.6",
    "eslint": "^9.23.0",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "new-github-release-url": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.66.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-preset-solid": "^3.0.0",
    "storybook": "^9.0.15",
    "storybook-solidjs-vite": "^9.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
